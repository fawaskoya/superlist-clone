  "scripts": {
    "dev": "cp prisma/schema.sqlite.prisma prisma/schema.prisma && NODE_ENV=development nodemon --exec tsx server/index.ts",
    "build": "cp prisma/schema.postgresql.prisma prisma/schema.prisma && npx vite build && npx esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "prisma db push",
    "postinstall": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:dev": "cp prisma/schema.sqlite.prisma prisma/schema.prisma && prisma db push --accept-data-loss"
  },
